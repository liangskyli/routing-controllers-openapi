{
  "name": "@liangskyli/routing-controllers-openapi",
  "version": "0.8.3",
  "description": "routing-controllers generate openapi file",
  "author": "liangsky <640634387@qq.com>",
  "homepage": "https://github.com/liangskyli/routing-controllers-openapi#readme",
  "license": "MIT",
  "keywords": [
    "routing-controllers",
    "openapi",
    "routing-controllers generate openapi file"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "import": "./lib/index.js",
        "default": "./lib/index.js"
      }
    }
  },
  "files": [
    "lib",
    "bin",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "bin": {
    "gen-openapi": "bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liangskyli/routing-controllers-openapi.git"
  },
  "scripts": {
    "test": "vitest run",
    "test-u": "vitest -u run",
    "coverage": "vitest run --coverage",
    "build": "rollup --config=./rollup.config.js",
    "update:deps": "pnpm update --interactive --latest"
  },
  "dependencies": {
    "@liangskyli/utils": "7.0.0",
    "commander": "^13.1.0",
    "glob": "^10.4.5",
    "js-yaml": "^4.1.1",
    "openapi3-ts": "^4.5.0",
    "typescript": "~5.5.0",
    "typescript-json-schema": "^0.66.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/koa": "^3.0.1",
    "koa": "^3.1.1",
    "routing-controllers": "^0.11.3",
    "typedi": "^0.10.0"
  },
  "peerDependencies": {
    "routing-controllers": ">=0.9.0"
  },
  "peerDependenciesMeta": {
    "routing-controllers": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": {
    "url": "https://github.com/liangskyli/routing-controllers-openapi/issues"
  }
}
